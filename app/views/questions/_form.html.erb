
<div class="container">
  <%= form_with  model: @question, local: true do |f| %>

     <div>
       <%= f.label :title %>
       <%= f.text_field :title %>
     </div>

     <div>
       <%= f.label :body %>
       <%= f.text_area :body %>
     </div>

     <div><%= f.submit %></div>

  <% end %>
</div>
